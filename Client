import React from 'react';
import './App.css';

class App extends React.Component {
  state = {
    users: [],
  };

  componentDidMount() {
    fetch('/users')
      .then(res => res.json()) 
      .then(users => this.setState({ users }))
  }

  render() {
  return (
    <div className="App">
      <h1>Users</h1>
      {this.state.users.map(user=> 
        <li key={user.id}>
          {user.username}
        </li>)
      }
    </div>
  );
}
}

export default App;
